{
  "name": "meta-scraper",
  "version": "1.0.0",
  "description": "A Javascript library to easily parse the metadata from a web page",
  "main": "lib/meta-scraper.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/webpack-library-starter.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trapajim/meta-scraper.git"
  },
  "keywords": [
    "metadata",
    "parser",
    "page"
  ],
  "author": "Tobias Koch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trapajim/meta-scraper/issues"
  },
  "homepage": "https://github.com/trapajim/meta-scraper/",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "domino": "^2.1.3",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "jsdom": "16.5.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "yargs": "^13.2.4"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {}
}
